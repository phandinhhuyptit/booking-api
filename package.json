{
  "name": "cms-elastic-service",
  "version": "1.0.0",
  "description": "Content Management System and Elasticsearch Buzz",
  "main": "index.js",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "pretest": "eslint src",
    "start": "node dist/index.js",
    "build": "NODE_ENV=production && rm -rf dist/ & babel src --out-dir dist --copy-files",
    "dev": "nodemon --exec babel-node src/index.js",
    "index:master": "nodemon --exec babel-node src/services/generateMappingMasterBuzz.js",
    "index:topic": "nodemon --exec babel-node src/services/generateMappingTopicBuzz.js",
    "index:profile": "nodemon --exec babel-node src/services/generateMappingProfile.js",
    "bulk:master": "nodemon --exec babel-node src/schedules/bulk.master.js",
    "bulk:topic": "nodemon --exec babel-node src/schedules/bulk.topic.js",
    "bulk:profile": "nodemon --exec babel-node src/schedules/bulk.profile.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@elastic/elasticsearch": "^7.6.1",
    "apollo-datasource": "^0.7.1",
    "apollo-server": "^2.14.2",
    "apollo-server-cache-redis": "^1.2.1",
    "apollo-server-errors": "^2.4.1",
    "apollo-server-express": "^2.13.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.2",
    "graphql-tools": "^5.0.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "moment": "^2.25.3",
    "mongoose": "^5.9.18",
    "sift": "^13.1.10",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^0.14.3",
    "nodemon": "^2.0.3",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0"
  }
}
